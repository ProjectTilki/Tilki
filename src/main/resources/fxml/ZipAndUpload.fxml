<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="CENTER" hgap="20.0" vgap="20.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kasirgalabs.tilki.client.ZipAndUploadController">
    <stylesheets>
        <URL value="@/styles/tilki.css" />
    </stylesheets>
    <columnConstraints>
        <ColumnConstraints hgrow="NEVER" minWidth="20.0" />
        <ColumnConstraints hgrow="NEVER" maxWidth="360.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints vgrow="NEVER" />
        <RowConstraints minHeight="20.0" vgrow="NEVER" />
        <RowConstraints vgrow="NEVER" />
        <RowConstraints vgrow="NEVER" />
        <RowConstraints vgrow="NEVER" />
        <RowConstraints vgrow="NEVER" />
    </rowConstraints>
    <children>
        <Label id="infoLabel" fx:id="messageLabel" text="Label" GridPane.columnSpan="2" GridPane.halignment="CENTER" />
        <fx:include source="ProgressLabel.fxml" GridPane.columnSpan="2" GridPane.rowIndex="2" />
        <fx:include source="ProgressBar.fxml" GridPane.columnSpan="2" GridPane.rowIndex="3" />
        <fx:include source="ProgressMessageTextArea.fxml" GridPane.columnSpan="2" GridPane.rowIndex="4" />
        <Button mnemonicParsing="false" text="Button" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="5" />
        <Label text="Label" GridPane.columnSpan="2" GridPane.rowIndex="5" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</GridPane>
